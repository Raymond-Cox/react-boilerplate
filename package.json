{
    "name": "react-boilerplate",
    "version": "1.0.0",
    "description": "A template for React projects using Parcel",
    "source": "src/index.html",
    "browserslist": "> 0.5%, last 2 versions, not dead",
    "scripts": {
        "start": "parcel --open",
        "clean": "rmdir /s dist",
        "build": "parcel build --public-url ./",
        "test": "jest",
        "lint": "eslint .",
        "pretty": "prettier --write .",
        "pretty-check": "prettier --check ."
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Raymond-Cox/react-boilerplate.git"
    },
    "keywords": [
        "React",
        "Template",
        "Boilerplate"
    ],
    "author": "Raymond-Cox",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Raymond-Cox/react-boilerplate/issues"
    },
    "homepage": "https://github.com/Raymond-Cox/react-boilerplate#readme",
    "dependencies": {
        "axios": "0.27.2",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@babel/core": "^7.20.5",
        "@babel/preset-react": "^7.18.6",
        "@parcel/babel-plugin-transform-runtime": "^2.8.1",
        "@parcel/babel-preset-env": "^2.8.1",
        "@testing-library/react": "^13.4.0",
        "@types/jest": "^29.2.4",
        "babel-jest": "^29.3.1",
        "eslint": "^8.29.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-config-standard": "^17.0.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jest": "^27.1.6",
        "eslint-plugin-n": "^15.6.0",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-plugin-react": "^7.31.11",
        "jest": "^29.3.1",
        "jest-environment-jsdom": "^29.3.1",
        "jest-sonar": "^0.2.15",
        "jest-transform-stub": "^2.0.0",
        "parcel": "^2.8.2",
        "prettier": "2.8.1",
        "process": "^0.11.10"
    }
}
