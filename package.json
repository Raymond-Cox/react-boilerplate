{
    "name": "react-boilerplate",
    "version": "1.0.0",
    "description": "A template for React projects using Parcel",
    "source": "src/index.html",
    "browserslist": "> 0.5%, last 2 versions, not dead",
    "scripts": {
        "start": "parcel --open -p 3000",
        "clean": "rmdir /s dist",
        "build": "parcel build --public-url ./",
        "test": "jest",
        "lint": "eslint .",
        "pretty": "prettier --write .",
        "pretty-check": "prettier --check .",
        "prepare": "husky install"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Raymond-Cox/react-boilerplate.git"
    },
    "keywords": [
        "React",
        "Template",
        "Boilerplate"
    ],
    "author": "Raymond-Cox",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Raymond-Cox/react-boilerplate/issues"
    },
    "homepage": "https://github.com/Raymond-Cox/react-boilerplate#readme",
    "dependencies": {
        "axios": "^1.4.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@babel/core": "^7.22.1",
        "@babel/preset-react": "^7.22.3",
        "@parcel/babel-plugin-transform-runtime": "^2.9.1",
        "@parcel/babel-preset-env": "^2.9.1",
        "@testing-library/react": "^14.0.0",
        "@types/jest": "^29.5.2",
        "babel-jest": "^29.5.0",
        "eslint": "^8.42.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-config-standard": "^17.1.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-jest": "^27.2.1",
        "eslint-plugin-n": "^16.0.0",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-plugin-react": "^7.32.2",
        "husky": "^8.0.3",
        "jest": "^29.5.0",
        "jest-environment-jsdom": "^29.5.0",
        "jest-sonar": "^0.2.16",
        "jest-transform-stub": "^2.0.0",
        "lint-staged": "^13.2.2",
        "parcel": "^2.9.1",
        "prettier": "^2.8.8",
        "process": "^0.11.10"
    },
    "lint-staged": {
        "*.js": "eslint .",
        "*.{js,css,md,json}": "prettier --write"
    }
}
